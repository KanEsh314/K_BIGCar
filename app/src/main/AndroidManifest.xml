<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="my.com.itrain.big_car">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/google_maps_key" />

        <activity android:name=".MainActivity" />
        <activity android:name=".EditProfileActivity" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".SignUpActivity" />
        <activity android:name=".TourDetailActivity" />
        <activity android:name=".TourDatesActivity" />
        <activity android:name=".TourConfirmActivity" />
        <activity android:name=".TourCountActivity" />
        <activity android:name=".StartActivity" />
        <activity android:name=".ResetPasswordActivity" />
        <activity android:name=".CategoryActivity" />
        <activity android:name=".ConditionActivity" />
        <activity android:name=".AccountActivity" />
        <activity android:name=".ContactActivity" />
        <activity android:name=".TourSummaryActivity" />
        <activity android:name=".FavoriteActivity" />
        <activity android:name=".SearchActivity" />
        <activity android:name=".BookingActivity"></activity>
    </application>

</manifest>